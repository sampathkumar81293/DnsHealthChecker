{
	"manifest_version" : 2,

	"name" : "DNS Health Checker",
	"description" : "Plugin which detects DNS spoofing and alerts the user",
	"version" : "0.0.1",
	"icons":
	{
		"128": "icon.png"
    },

    "background" : {
        "scripts" : ["background.js"],
        "persistant"  : "true"
    },
    "permissions" : [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}